{{> default/header }}
<body id="top">
    <!-- preloader -->
    {{> default/loader }}
    <!-- header -->
    {{> default/navbar}}
    <!-- featured  -->
    {{> default/feature-slider}}
    <!-- s-content -->
    <section class="s-content">
      <div class="row entries-wrap wide">
        <div class="entries">
          {{> default/post}}
          {{> default/post}}
          {{> default/post}}
          {{> default/post}}
        </div> <!-- end entries -->
      </div> <!-- end entries-wrap -->
      {{> default/pagination}}
    </section> <!-- end s-content -->
    <!-- s-extra -->
    <section class="s-extra">
      <div class="row">
      <div class="col-twelve md-twelve tab-full popular">
        <h3>Popular Posts</h3>
        <div class="block-1-2 block-m-full popular__posts">
          {{> default/popular-post}}
          {{> default/popular-post}}
          {{> default/popular-post}}
          {{> default/popular-post}}
          {{> default/popular-post}}
          {{> default/popular-post}}
        </div> <!-- end popular_posts -->
      </div> <!-- end popular -->
      </div> <!-- end row -->
    </section> <!-- end s-extra -->

    <!-- s-footer -->
    {{> default/footer}}
    <!-- Javascript -->
    <script src="/default/js/jquery-3.2.1.min.js"></script>
    <script src="/default/js/plugins.js"></script>
    <script src="/default/js/main.js"></script>
</body>
</html>